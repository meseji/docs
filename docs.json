{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Meseji Documentation",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": {
    "light": "/images/favicon icon.png",
    "dark": "/images/favicon icon.png"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "href": "https://docs.meseji.app"
        },
        {
          "anchor": "Community",
          "icon": "slack",
          "href": "https://meseji.app/community"
        },
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://meseji.app/blog"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development",
              "guides/getting-started/pricing"
            ]
          },
          {
            "group": "Waba Management",
            "pages": [
              "guides/bsp-migrating"
            ]
          },
          {
            "group": "Solutions",
            "pages": [
              "guides/solutions/e-commerce",
              "guides/solutions/healthcare"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "https://api.meseji.app/api/docs/json",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      },
      {
        "tab": "Integration",
        "pages": [
          "integration/introduction",
          "integration/shopify",
          "integration/woo-commerce"
        ]
      },
      {
        "tab": "FAQs"
      },
      {
        "tab": "Changelog",
        "pages": [
          "changelog/page"
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@meseji.app"
      },
      {
        "label": "Login",
        "href": "https://panel.meseji.app/login"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://panel.meseji.app"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mesejiOfficial",
      "linkedin": "https://www.linkedin.com/company/meseji",
      "instagram": "https://www.instagram.com/meseji.app"
    }
  },
  "api": {
    "playground": {
      "display": "interactive"
    }
  },
  "seo": {
    "indexing": "navigable"
  }
}
